{
  "name": "prettier-plugin-sort-json",
  "version": "0.0.2",
  "private": false,
  "description": "Prettier plugin to sort JSON files alphanumerically by key",
  "repository": {
    "type": "git",
    "url": "https://github.com/Gudahtt/prettier-plugin-sort-json.git"
  },
  "license": "MIT",
  "author": "Mark Stacey <markjstacey@gmail.com>",
  "main": "index.js",
  "files": [
    "index.js",
    "index.d.ts",
    "index.js.map"
  ],
  "scripts": {
    "setup": "yarn install && yarn allow-scripts",
    "build": "tsc --project .",
    "prepublishOnly": "yarn build",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint:eslint": "eslint . --cache --ext js,ts",
    "lint:misc": "prettier '**/*.json' '**/*.md' '!CHANGELOG.md' '**/*.yml' --ignore-path .gitignore",
    "lint": "yarn lint:eslint && yarn lint:misc --check",
    "lint:fix": "yarn lint:eslint --fix && yarn lint:misc --write"
  },
  "dependencies": {
    "@types/prettier": "^2.2.0"
  },
  "devDependencies": {
    "@lavamoat/allow-scripts": "^1.0.6",
    "@metamask/eslint-config": "^7.0.1",
    "@metamask/eslint-config-jest": "^7.0.0",
    "@metamask/eslint-config-nodejs": "^7.0.1",
    "@metamask/eslint-config-typescript": "^7.0.1",
    "@types/jest": "^26.0.13",
    "@typescript-eslint/eslint-plugin": "^4.28.4",
    "@typescript-eslint/parser": "^4.28.4",
    "eslint": "^7.31.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-jest": "^24.3.6",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.4.0",
    "jest": "^26.4.2",
    "prettier": "^2.3.2",
    "prettier-plugin-packagejson": "^2.2.11",
    "ts-jest": "^26.3.0",
    "typescript": "^4.1.5"
  },
  "peerDependencies": {
    "prettier": "^2.1.0"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "lavamoat": {
    "allowScripts": {
      "@lavamoat/preinstall-always-fail": false
    }
  }
}
